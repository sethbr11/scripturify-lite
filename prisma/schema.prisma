generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Scripture {
  id        Int     @id @default(autoincrement())
  ref       String
  text      String
  keywords  String
}

model Project {
  id         Int      @id @default(autoincrement())
  title      String
  userText   String
  footnotes  String
  createdAt  DateTime @default(now())
}
